{
	"appId": "com.zivoramobile.zivora",
	"appName": "Zivora",
	"webDir": "dist/public",
	"server": {
		"androidScheme": "https",
		"allowNavigation": [
			"*"
		],
		"cleartext": true
	},
	"android": {},
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 2000,
			"backgroundColor": "#1a1a2e",
			"androidScaleType": "CENTER_CROP",
			"showSpinner": false,
			"splashFullScreen": true,
			"splashImmersive": false
		}
	}
}
